[Unit]
Description=NubeIO LoRa Raw pyserial
After=network.target

[Service]
Type=simple
User=<user>
WorkingDirectory=<working_dir>
ExecStart=<lib_dir>/venv/bin/gunicorn -b 0.0.0.0:1919 --log-level=DEBUG --worker-class gevent -w 1 run:app -e data_dir=/data/lora-raw
Restart=always
RestartSec=10
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=lora-raw

[Install]
WantedBy=multi-user.target
