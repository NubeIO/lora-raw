{
  "mqtt": {
    "enabled": true,
    "name": "lora-raw-mqtt",
    "host": "0.0.0.0",
    "port": 1883,
    "keepalive": 60,
    "qos": 1,
    "retain": false,
    "attempt_reconnect_on_unavailable": true,
    "attempt_reconnect_secs": 5,
    "topic": "rubix/lora_raw/value",
    "publish_raw": true,
    "raw_topic": "rubix/lora_raw/raw",
    "publish_debug": true,
    "debug_topic": "rubix/lora_raw/debug"
  },
  "serial": {
    "enabled": true,
    "port": "/dev/ttyUSB0",
    "baud_rate": 9600,
    "stop_bits": 1,
    "parity": "N",
    "byte_size": 8,
    "timeout": 5
  }
}
